      "preLaunchTask": "build.unittest",
      "program": "${workspaceFolder}/CSBP.UnitTest/bin/Debug/net8.0/CSBP.UnitTest.dll",
      "args": [
        "DB_DRIVER_CONNECTxxx=Data Source=/home/wolfgang/Dokumente/Rainbows/hsqldb/csbp-rainbow.db"
